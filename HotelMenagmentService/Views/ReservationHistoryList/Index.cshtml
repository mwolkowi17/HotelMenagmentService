@model HotelMenagmentService.Models.HotelViewModel

@{
    ViewData["Title"] = "Index";
}

<h1>Reservation History List</h1>
<table class="table">
    <thead>
        <tr>


            <th>
                ReservationID
            </th>
            <th>
                check in
            </th>
            <th>
                check out
            </th>
           
            <th>
                Guest ID
            </th>
            <th>
                Guest Name
            </th>
            <th>
                Room ID
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ReservationHistoryList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.ReservationHistoryID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.check_in_History)
                </td>
                <td>
                    @Html.DisplayFor(modleItem => item.check_out_History)
                </td>
                
                <td>
                    @Html.DisplayFor(modelItem => item.GuestID_History)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GuestName_History)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RoomID_History)
                </td>
                
            </tr>
        }
    </tbody>
 </table>
